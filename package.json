{
  "name": "nel-3d",
  "version": "0.0.0",
  "description": "NeL 3D library",
  "main": "src/nel/3d",
  "registry": "github",
  "scripts": {
    "lint": "jshint src/nel/3d/**/*.js test/nel/3d/**/*.js",
    "pretest": "npm run lint",
    "test": "mocha --recursive --compilers js:babel/register",
    "postinstall": "jspm install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zerotacg/nel-3d.git"
  },
  "author": {
    "name": "Tobias Peters",
    "email": "tobias.peters@kreativeffekt.at"
  },
  "contributors": [
    {
      "name": "Tobias Peters",
      "email": "tobias.peters@kreativeffekt.at"
    }
  ],
  "license": "MIT",
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/zerotacg/nel-3d/blob/master/LICENSE"
    }
  ],
  "bugs": {
    "url": "https://github.com/zerotacg/nel-3d/issues"
  },
  "homepage": "https://github.com/zerotacg/nel-3d#readme",
  "devDependencies": {
    "babel": "^5.8.24",
    "chai": "^3.4.1",
    "jshint": "^2.8.0",
    "jspm": "^0.16.15",
    "mocha": "^2.3.4",
    "sinon": "^1.17.2",
    "sinon-chai": "^2.8.0"
  },
  "jspm": {
    "name": "nel/3d",
    "directories": {
      "lib": "src/nel/3d"
    },
    "dependencies": {
      "nel/io": "zerotacg/nel-io@master",
      "nel/misc": "zerotacg/nel-misc@master",
      "three": "mrdoob/three.js@master"
    },
    "devDependencies": {
      "babel": "npm:babel-core@^5.8.24",
      "babel-runtime": "npm:babel-runtime@^5.8.24",
      "core-js": "npm:core-js@^1.1.4"
    },
    "overrides": {
      "github:zerotacg/nel-io@master": {
        "directories": {
          "lib": "src/nel/io"
        }
      },
      "github:zerotacg/nel-misc@master": {
        "directories": {
          "lib": "src/nel/misc"
        }
      }
    }
  }
}
